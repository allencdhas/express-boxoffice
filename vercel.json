{
  "version": 2,
  "builds": [
    {
      "src": "boxoff.js",
      "use": "@vercel/node"
    },
    {
      "src": "requirements.txt",
      "use": "@vercel/python"
    },
    {
      "src": "boxoff.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "boxoff.js"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.10",
    "PYTHONPATH": "."
  }
} 